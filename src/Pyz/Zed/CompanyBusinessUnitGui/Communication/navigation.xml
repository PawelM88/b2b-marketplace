<?xml version="1.0" encoding="UTF-8"?>
<config>
    <company-gui>
        <label>Company Account</label>
        <title>Company Account</title>
        <icon>fa-files-o</icon>
        <pages>
            <company-business-unit-gui>
                <label>Business Units</label>
                <title>Business Units</title>
                <bundle>company-business-unit-gui</bundle>
                <controller>list-company-business-unit</controller>
                <action>index</action>
                <pages>
                    <import-company-business-unit>
                        <label>Import Company Business Units</label>
                        <title>Import Company Business Units</title>
                        <bundle>company-business-unit-gui</bundle>
                        <controller>import</controller>
                        <action>index</action>
                        <visible>0</visible>
                        <pages>
                            <upload-company-business-unit>
                                <label>Upload Company Business Units</label>
                                <title>Upload Company Business Units</title>
                                <bundle>company-business-unit-gui</bundle>
                                <controller>import</controller>
                                <action>upload</action>
                                <visible>0</visible>
                            </upload-company-business-unit>
                        </pages>
                    </import-company-business-unit>
                </pages>
            </company-business-unit-gui>
        </pages>
    </company-gui>
</config>
