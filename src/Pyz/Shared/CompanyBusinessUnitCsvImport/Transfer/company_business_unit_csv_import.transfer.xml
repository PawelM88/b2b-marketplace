<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CompanyBusinessUnitCsvValidationResult">
        <property name="isSuccess" type="bool"/>
        <property name="error" type="string"/>
    </transfer>

    <transfer name="CompanyBusinessUnitListImportResponse">
        <property name="companyBusinessUnitList" type="CompanyBusinessUnitList"/>
        <property name="errors" type="CompanyBusinessUnitListImportError[]" singular="error"/>
        <property name="companyBusinessUnitListImportRequest" type="CompanyBusinessUnitListImportRequest"/>
        <property name="isSuccess" type="bool"/>
    </transfer>

    <transfer name="CompanyBusinessUnitList">
        <property name="idCompanyBusinessUnitList" type="int"/>
        <property name="name" type="string"/>
        <property name="createdAt" type="string"/>
        <property name="isActive" type="bool"/>
    </transfer>

    <transfer name="CompanyBusinessUnitListImportError">
        <property name="companyBusinessUnitImport" type="CompanyBusinessUnitImport"/>
        <property name="message" type="string"/>
        <property name="parameters" type="string[]" singular="parameter"/>
    </transfer>

    <transfer name="CompanyBusinessUnitListImportRequest">
        <property name="companyBusinessUnitList" type="CompanyBusinessUnitList"/>
        <property name="items" type="CompanyBusinessUnitImport[]" singular="item"/>
    </transfer>

    <transfer name="CompanyBusinessUnitImport">
        <property name="metaData" type="CompanyBusinessUnitImportMetaData"/>
        <property name="company" type="Company"/>
        <property name="companyBusinessUnit" type="CompanyBusinessUnit"/>
        <property name="companyUnitAddress" type="CompanyUnitAddress"/>
        <property name="companyBusinessUnit" type="CompanyBusinessUnit"/>
        <property name="companyUser" type="CompanyUser"/>
        <property name="customer" type="Customer"/>
    </transfer>

    <transfer name="CompanyBusinessUnitImportMetaData">
        <property name="identifier" type="string"/>
    </transfer>

    <transfer name="Company">
        <property name="isNew" type="bool"/>
        <property name="storeName" type="string"/>
        <property name="companyBusinessUnit" type="CompanyBusinessUnit"/>
        <property name="companyUnitAddress" type="CompanyUnitAddress"/>
    </transfer>

    <transfer name="CompanyBusinessUnit">
        <property name="parentCompanyBusinessUnitKey" type="string"/>
        <property name="isNew" type="bool"/>
        <property name="isUpdated" type="bool"/>
    </transfer>

    <transfer name="CompanyUnitAddress">
        <property name="state" type="string"/>
        <property name="isNew" type="bool"/>
        <property name="isUpdated" type="bool"/>
    </transfer>

    <transfer name="Customer">
        <property name="isNew" type="bool"/>
        <property name="isUpdated" type="bool"/>
    </transfer>

    <transfer name="ImportStatistic">
        <property name="updatedCompanyBusinessUnit" type="int"/>
        <property name="newCompanyBusinessUnit" type="int"/>
        <property name="updatedCompanyUnitAddress" type="int"/>
        <property name="newCompanyUnitAddress" type="int"/>
        <property name="updatedAdmin" type="int"/>
        <property name="newAdmin" type="int"/>
    </transfer>

</transfers>
