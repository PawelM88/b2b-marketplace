<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ContactForm">
        <property name="salutation" type="string"/>
        <property name="firstName" type="string"/>
        <property name="lastName" type="string"/>
        <property name="phoneNumber" type="int"/>
        <property name="email" type="string"/>
        <property name="subject" type="string"/>
        <property name="message" type="string"/>
        <property name="merchantEmail" type="string"/>
        <property name="merchantName" type="string"/>
        <property name="merchantProfileCriteria" type="MerchantProfileCriteria"/>
    </transfer>

    <transfer name="ContactFormResponse">
        <property name="isSuccess" type="bool"/>
        <property name="errorMessage" type="string"/>
    </transfer>

    <transfer name="Mail">
        <property name="contactForm" type="ContactForm"/>
    </transfer>
</transfers>
